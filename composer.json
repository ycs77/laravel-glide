{
    "name": "ycs77/laravel-glide",
    "description": "Glide image manipulation for Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Yang",
            "email": "yangchenshin77@gmail.com"
        }
    ],
    "keywords": [
        "laravel",
        "glide",
        "image",
        "manipulation"
    ],
    "homepage": "https://github.com/ycs77/laravel-glide",
    "require": {
        "php": "^8.0.2",
        "illuminate/http": ">=5.4",
        "illuminate/support": ">=5.4",
        "league/glide": "^2.2",
        "league/glide-symfony": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Ycs77\\LaravelGlide\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ycs77\\LaravelGlide\\GlideServiceProvider"
            ],
            "aliases": {
                "Glide": "Ycs77\\LaravelGlide\\Facades\\Glide"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
